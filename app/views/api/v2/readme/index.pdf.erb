<h1>T27DUCK'S README</h1>

<hr />

<h3>Quick Info (TLDR)</h3>
<ul>
  <% @entries.each do |entry| %>
    <li><%= entry.content %></li>
  <% end %>
</ul>

<hr />

<% @user_manual_headings.each do |user_manual_heading| %>
  <h3><%= user_manual_heading.content %></h3>
  <ul>
    <% user_manual_heading.user_manual_entries.each do |entry| %>
      <li><%= entry.content %></li>
    <% end %>
  </ul>
<% end %>

<hr />

<h3>Where to Find Me Online</h3>
<ul>
  <% @links.each do |link| %>
    <li><%= link.link_type %>: <%= link_to link.url, link.url, target: '_blank' %></li>
  <% end %>
</ul>

<hr />

<h3>Meta</h3>
<ul>
  <li>Information Last Updated: <%= @last_updated.to_formatted_s(:long_ordinal) %></li>
  <li>Source Code: <%= link_to @source, @source, target: '_blank' %></li>
  <li>Homepage: <%= link_to @homepage, @homepage, target: '_blank' %></li>
</ul>
